<h1>Edit Schedule</h1>
<div class="pagination">
     <%= will_paginate @schedules, :container => false %>
</div>
<table>
<% @schedules.each do |event| %>
	<%= render "event", event: event %>
<% end %>
</table>
<div class="pagination">
     <%= will_paginate @offerings, :container => false %>
</div>
<script>
$(function() {
	 $( ".autocomplete" ).autocomplete({
			source: $( "#<%= @schedules.first.id %>" ).data('autocomplete-source')
	});
});
</script>
