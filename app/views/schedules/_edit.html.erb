	<%= form_tag edit_schedule_path(schedule), method: "get", class: "form-horizontal" do %>

		<div class="form-group">
			<label for="<%= schedule.id %>" class="col-sm-4 control-label">Project</label>
			<div class="col-sm-8">
				<%= text_field_tag :project_name, (schedule.project.name if schedule.project), data: {autocomplete_source: autocomplete_projects_path}, class: "autocomplete form-control", id: schedule.id %>
			</div>
			</div>

		<div class="form-group">
			<label for="start_at<%= schedule.id %>" class="col-sm-4 control-label">Start Time</label>
			<div class="datetimepicker col-sm-8" data-date-format="MM/DD/YYYY hh:mm A">
	    <%= text_field_tag :start_at, (schedule.start_at.strftime('%m/%d/%Y %I:%M %p') if schedule.start_at), id: "start_at#{schedule.id}", class: "form-control"  %>
	  </div>
	  </div>
	  <div class="form-group">
			<label for="end_at<%= schedule.id %>" class="col-sm-4 control-label">End Time</label>
			<div class="datetimepicker col-sm-8" data-date-format="MM/DD/YYYY hh:mm A">
	    <%= text_field_tag :end_at, (schedule.end_at.strftime('%m/%d/%Y %I:%M %p') if schedule.end_at), id: "end_at#{schedule.id}", class: "form-control"  %>
	  </div>
	  </div>
		<div class="modal-footer">

			<%= submit_tag"Save changes", class: "btn btn-default" %>
			<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
		</div>
	<% end %>


